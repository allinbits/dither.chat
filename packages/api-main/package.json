    "@keplr-wallet/cosmos": "^0.12.280",
    "@sinclair/typebox": "^0.34.41",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.43.1",
    "drizzle-typebox": "^0.3.3",
    "elysia": "~1.2.25",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@cosmjs/amino": "^0.33.1",
    "@types/jsonwebtoken": "^9.0.10",
